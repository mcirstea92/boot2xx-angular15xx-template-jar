/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "paths": {
      "path": [
        "./node_modules/stream-browserify"
      ]
    },
    "types": [
      "node",
      "jquery"
    ],
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts",
    "node_modules/@types/jquery/misc.d.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
